{#
 	Title: Breadcrumbs
 	Description: Breadcrumbs for given page
 	Category: formatting
 	Icon: admin-comments
 	Keywords: breadcrumbs
 	Mode: edit
 	Align: full
 	PostTypes: page post jobs
 	SupportsAlign: left right full
 	SupportsMode: true
 	SupportsMultiple: true
#}


<section class="Breadcrumbs container h-10 justify-start items-center flex mx-auto mb-[5rem]">
	<div class="Frame27  justify-start items-center gap-2.5 flex mr-[1.25rem] hover:border-b-4 border-cyan-950">
		<div class="Home w-6 h-6 relative">
			<svg width="24" height="24" viewbox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
				<g id="home">
					<path id="Vector" d="M3 9L12 2L21 9V20C21 20.5304 20.7893 21.0391 20.4142 21.4142C20.0391 21.7893 19.5304 22 19 22H5C4.46957 22 3.96086 21.7893 3.58579 21.4142C3.21071 21.0391 3 20.5304 3 20V9Z" stroke="#1E1E1E" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
					<path id="Vector_2" d="M9 22V12H15V22" stroke="#1E1E1E" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
				</g>
			</svg>
		</div>
		<div class="Home text-stone-900 text-lg font-weight: 500 font-['Red Hat Mono'] uppercase leading-[30px]">
			<a href={{ home_page_url }}>Home</a>
		</div>
	</div>
	{% if parent_page_title %}
		<div class="Line8 w-[30px] h-[0px] origin-top-left -rotate-90 border border-stone-900 mt-7"></div>
		<div class="Frame28  justify-start items-center gap-2.5 flex mr-[1.25rem]">
			<div class="Blog text-stone-900 text-lg font-weight: 500 font-['Red Hat Mono'] uppercase leading-[30px] hover:border-b-4 border-cyan-950">
				<a href={{ parent_page_url}}>{{ parent_page_title }}</a>
			</div>
		</div>
	{% endif %}
	{% if page_title %}
		<div class="Line9 w-[30px] h-[0px] origin-top-left -rotate-90 border border-stone-900 mt-7"></div>
		<div class="Frame30  border-b-4 border-cyan-950 justify-start items-center gap-2.5 flex">
			<div class="FlatDesign text-stone-900 text-lg font-bold font-['Red Hat Mono'] uppercase leading-[30px]">{{ page_title }}</div>
		</div>
	{% endif %}
</section>





